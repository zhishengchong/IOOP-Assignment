<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<connectionStrings>
		<add name="ARFMS"
			 connectionString="Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=ARFMS;Integrated Security=True;MultipleActiveResultSets=True"
			 providerName="System.Data.SqlClient" />
